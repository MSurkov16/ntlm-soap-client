{
    "name": "msurkov16/ntlm-soap-client",
    "type": "library",
    "description": "PHP WDSL/Soap Client with NTLM Authentication for Microsoft Dynamics NAT Server.",
    "keywords": [
        "matejsvajger",
        "ntlm-soap-client"
    ],
    "homepage": "https://github.com/MSurkov16/ntlm-soap-client/",
    "license": "MIT",
    "authors": [
        {
            "name": "Maksim Surkov",
            "email": "mssurkov16@gmail.com",
            "homepage": "https://www.webranding.ru",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "~5.6|~8.0",
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "matejsvajger\\NTLMSoap\\": "src/NTLMSoap/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "matejsvajger\\NTLMSoap\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/NTLMSoap"
    ]
}
